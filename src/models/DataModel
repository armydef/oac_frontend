
class DataModel {

    constructor(req, options = {}){
        this.user = req.user || {};
        this.options = options;
    }

    toJson(){  
        let json = { 
            user: this.user, 
            cur_id: this.user.id || 0,
            ...this.options 
        };
        console.log(json)
        return json
    }

}

module.exports = DataModel;